#		*Translator Definitions*
CC = gcc
LINK = gcc
#OBJMODE = -Zomf -Zsys
OBJMODE = -Zomf -Zcrtdll
CFLAGS = -c $(OBJMODE) -Wall -Zmt -O2 -D__OS2__ -Ilib -s -funsigned-char
LFLAGS = -Zmt $(OBJMODE) -Wall -Zstack 512 -s -lwrap
OBJDIR = obj_emx
TCP_INCLUDE = /watcom/include

ifdef DEBUG
CFLAGS = -c -Wall -Zmt -O2 -D__OS2__ -Ilib -g
LFLAGS = -Zmt -Wall -Zstack 512 -g
OBJDIR = obj_emx/debug
LXLITE = @rem
else
OBJDIR = obj_emx
LXLITE = lxlite
endif

